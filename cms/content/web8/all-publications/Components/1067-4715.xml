<PackageItem xmlns:tcm="http://www.tridion.com/ContentManager/5.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.sdltridion.com/ContentManager/ImportExport/Package/2013">
  <Data>
    <tcm:Data>
      <tcm:Title>Item List</tcm:Title>
      <tcm:Type>Normal</tcm:Type>
      <tcm:Schema xlink:type="simple" xlink:title="Item List" xlink:href="/webdav/110%20DXA%20Site%20Type/Building%20Blocks/Modules/Core/Editor/Schemas/Item%20List.xsd" IsMandatory="false" />
      <tcm:Content>
        <ItemList xmlns="http://www.sdl.com/web/schemas/core">
          <headline>Item List Heading</headline>
          <itemListElement>
            <subheading>A list of items</subheading>
            <content>That can be used in for instance a carousel with images and links.</content>
            <media xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="/webdav/110%20DXA%20Site%20Type/Building%20Blocks/Content/_Cloneable%20Content/Large%20Image.jpg" xlink:title="Large Image" />
            <link>
              <internalLink xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="/webdav/110%20DXA%20Site%20Type/Building%20Blocks/Content/_Cloneable%20Content/Article.xml" xlink:title="Article" />
            </link>
          </itemListElement>
          <itemListElement>
            <subheading>Second item in the list</subheading>
            <content>
              <p xmlns="http://www.w3.org/1999/xhtml">Praesent eget enim risus. Maecenas posuere risus eget turpis varius luctus.</p>
            </content>
            <media xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="/webdav/110%20DXA%20Site%20Type/Building%20Blocks/Content/_Cloneable%20Content/Another%20Large%20Image.jpg" xlink:title="Another Large Image" />
            <link>
              <linkText>External Link</linkText>
              <externalLink xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://www.sdl.com" />
            </link>
          </itemListElement>
        </ItemList>
      </tcm:Content>
      <tcm:Metadata>
        <Metadata xmlns="http://www.sdl.com/web/schemas/core" xmlns:xlink="http://www.w3.org/1999/xlink" />
      </tcm:Metadata>
      <tcm:IsBasedOnTridionWebSchema>true</tcm:IsBasedOnTridionWebSchema>
      <tcm:MultimediaType xlink:type="simple" xlink:title="" xlink:href="tcm:0-0-0" />
    </tcm:Data>
  </Data>
